<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="PedidoItem" table="PedidoItem">
    <id name="Id" column="IdItem" type="long">
      <generator class="identity" />
    </id>

    <property name="Cantidad" />
    <property name="PrecioUnitario" />

    <many-to-one name="Pedido" class="Pedido" column="IdPedido" />
    <many-to-one name="Producto" class="Producto" column="IdProducto" />
  </class>
</hibernate-mapping>