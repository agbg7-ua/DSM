<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Valoracion" table="Valoracion">
    <id name="Id" column="IdValoracion" type="long">
      <generator class="identity" />
    </id>

    <property name="Puntuacion" />
    <property name="Comentario" />

    <many-to-one name="Usuario" class="Usuario" column="IdUsuario" />
    <many-to-one name="Producto" class="Producto" column="IdProducto" />
  </class>
</hibernate-mapping>