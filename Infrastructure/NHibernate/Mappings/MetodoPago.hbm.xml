<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="MetodoPago" table="MetodoPago">
    <id name="IdMetodo" column="IdMetodo" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>

    <property name="Tipo" />
    <property name="Numero" />
    <property name="FechaExpiracion" type="DateTime" />

    <many-to-one name="Usuario" class="Usuario" column="IdUsuario" not-null="false" />

    <set name="Pedidos" inverse="true" cascade="all">
      <key column="IdMetodo" />
      <one-to-many class="Pedido" />
    </set>
  </class>
</hibernate-mapping>