<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Rol" table="Rol">
    <id name="IdRol" column="IdRol" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Nombre" />
    <property name="Descripcion" />
    <property name="Asociaciones" />

    <set name="Usuarios" inverse="true" cascade="all">
      <key column="IdRol" />
      <one-to-many class="Usuario" />
    </set>
  </class>
</hibernate-mapping>